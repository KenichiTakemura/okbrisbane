<% if top_page_ajaxable? %>
<% script = "" %>
<% [:p_job,:p_buy_and_sell,:p_estate,:p_business,:p_motor_vehicle,:p_accommodation,:p_law,:p_study].each do |category| %>
<% script += %Q|
	$('\#feed_body_#{category}').html("| + escape_javascript(image_tag("common/ajax_loading_1.gif")) + %Q|");
	$.post("| + top_feed_homes_path + %Q|", {c: '#{category}'});
|%>
<% end %>
<%= _script_document_ready(script) %>
<% end %>
