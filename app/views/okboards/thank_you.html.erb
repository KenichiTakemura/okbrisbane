<div id="okboards">
	<%= _left_side_menu %>
	<div id="page_section_right">
		<div id="content_head">
			<%= single_body_banner(5) %>
		</div>
		<div id="content_body">
			<div class="flash">
				<% if notice %>
				<p class="message notice">
					<%= notice %>
				</p>
				<% end %>
				<% if alert %>
				<p class="message alert">
					<%= alert %>
				</p>
				<% end %>
			</div>
			<%= render :partial => "okboards/#{Style.page(@okpage)}/thank_you" %>
		</div>
	</div>
</div>
<%= _script_document_ready(%Q|
$("#_#{@okpage}").attr("style", "opacity:1;filter:alpha(opacity=100);");
$('\#sub_menu_#{@okpage}').show("slow");
|) %>
