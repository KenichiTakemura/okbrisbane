<div style="position: relative; float: left; width: 750px;" id="board_for_<%= @okpage %>">
	<div class="okboard_top">
		<p class="okboard_title">
			<%= t(Style.page(@okpage))%>
		</p>
		<div class="okboard_title_image">
			<%= image_tag("#{I18n.locale}/#{Style.page(@okpage)}/view_bg.gif", :size => "750x100") %>
		</div>
		<div class="okboard_view_content">			
		<p class="post_deleted"><%= t(:this_post_has_been_deleted_by_admin)%></p>
		</div>
	</div>
</div>
